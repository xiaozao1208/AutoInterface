[
  {
    "description":"这是一个带header信息的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"aaa",
        "age":"333"
      }
    },
    "response":{
      "json":{
        "aaa":"success",
        "status":"1"
      },
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]